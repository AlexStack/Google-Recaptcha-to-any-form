{
    "name": "alexstack/google-recaptcha-to-any-form",
    "description": "Display a Google Recaptcha v2 or v3 or invisible in any custom form with flexible settings and no affection to your existing code. Also works well for SilverStripe 4.x/3.x/2.x & Laravel & Wordpress & other CMS. ",
    "homepage": "https://github.com/AlexStack/Google-Recaptcha-to-any-form",
    "type": "library",
    "keywords": [
      "silverstripe",
      "silverstripe3",
      "silverstripe4",
      "google recaptcha",
      "recaptcha",
      "google recaptcha v2"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alex",
            "homepage": "https://developers.google.com/recaptcha/"
        }
    ],
    "support": {
      "issues": "https://github.com/AlexStack/Google-Recaptcha-to-any-form/issues"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.0"
      },
      "autoload": {
        "psr-4": {
          "GoogleRecaptchaToAnyForm\\": "src/AlexStack/GoogleRecaptchaToAnyForm"
        }
      },
      "extra": {
        "screenshots": [
          "https://developers.google.com/recaptcha/images/newCaptchaAnchor.gif",
          "https://raw.githubusercontent.com/AlexStack/Google-Recaptcha-to-any-form/master/docs/images/google-recaptcha-v2-default.png",
          "https://raw.githubusercontent.com/AlexStack/Google-Recaptcha-to-any-form/master/docs/images/google-recaptcha-v3-default.png",
          "https://raw.githubusercontent.com/AlexStack/Google-Recaptcha-to-any-form/master/docs/images/google-recaptcha-v3-inline.png"
        ]
      }
}
